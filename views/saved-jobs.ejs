<h1>Your Saved Job Board:</h1>


<% saved.forEach(save => { %>
    <div>
        <h3><%= save.positionName %></h3>
        <h3><%= save.jobLink %></h3>
        <h3><%= save.company %></h3>
        <h3><%= save.location %></h3>
    
        <form method="POST" action="/users/job-board/:id" >
            <input hidden type="text" name="name" value="<%=save.name %>" >
        
            <button class="btn btn-prmary" type="submit">Add a Note</button>
        </form>
    </div>
<% }) %>